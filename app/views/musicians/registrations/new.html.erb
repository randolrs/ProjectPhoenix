<h2>Apply to be a SB Pro</h2>

<h3>Get high quality gig requests from local event planners </h3>

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: { class: 'form-horizontal'}) do |f| %>
  
  <%= f.error_notification %>

  <h4>Full name </h4>


  <h4>Mobile phone</h4>
  <%= f.input_field :phone %>

  <h4>Email address</h4>
  <%= f.input_field :email %>

  <h4>Category</h4>
 <%= f.select :music_type, [['a Band','Band'],['a DJ','DJ'],['Classical Musicians','Classical Musician']] %>

  <h4>Zip code</h4>
  <%= f.input_field :zip %>
  
  <h4>Password</h4>
  <%= f.input_field :password %>

  <h4>Confirm Password</h4>
  <%= f.input_field :password_confirmation %>
	


  <div class="form-actions">
    <%= f.submit "Submit", class: "btn btn-primary" %>
  </div>
  
<% end %>

<%= render "devise/shared/links" %>
